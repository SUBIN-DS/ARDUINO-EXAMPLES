int red = 3;
int blue = 2;
int a;
int x=4;
//int s=1;


  
void setup()
{   
  	Serial.begin(9600);
	pinMode(red, OUTPUT);
  	pinMode(blue, OUTPUT);

}
void loop(){
  
  for(a=1; a<=x; a=a+1)
  {
    Serial.println("Blue");
  	digitalWrite(blue, HIGH);
  	delay (500);
	digitalWrite(blue, LOW);
	delay (500);
}
	Serial.println("RED");
  	digitalWrite(red, HIGH);
  	delay (500);
	digitalWrite(red, LOW);
	delay (500);
}
