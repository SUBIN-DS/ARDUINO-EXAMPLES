int red=7;
int blue=10;
int var;

void setup()
{
  Serial.begin(9600);
  pinMode(red, OUTPUT);
  pinMode(blue, OUTPUT);
}

void loop()
{
  var=1;
  while(var<=5)
  {
    Serial.print("CHECKING RED LED # ");
    Serial.println(var);
 	digitalWrite(red, HIGH);
    delay(500);
    digitalWrite(red, LOW);
    delay(500);
    var++;
  }

   Serial.println("BLUE LOOP");
   digitalWrite(blue, HIGH);
   delay(500);
   digitalWrite(blue, LOW);
   delay(500);
}
